{
    "label" : "lock",
    "action" : "$HOME/dev/scripts/screenlock.sh && loginctl lock-session",
    "text" : "[L]ock",
    "keybind" : "l"
}
{
    "label" : "exit",
    "action" : "hyprctl dispatch exit",
    "text" : "[E]xit",
    "keybind" : "e"
}
{
    "label" : "hibernate",
    "action" : "$HOME/dev/scripts/screenlock.sh && systemctl suspend-then-hibernate",
    "text" : "[H]ibernate",
    "keybind" : "h"
}
{
    "label" : "shutdown",
    "action" : "systemctl poweroff",
    "text" : "[S]hutdown",
    "keybind" : "s"
}
{
    "label" : "reboot",
    "action" : "systemctl reboot",
    "text" : "[R]eboot",
    "keybind" : "r"
}
